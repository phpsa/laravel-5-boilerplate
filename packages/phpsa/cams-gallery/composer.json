{
  "name": "phpsa/cams-gallery",
  "description": "A Gallery component for the Phpsa CAMS system",
  "license": "MIT",
  "keywords": [
    "cms",
    " gallery",
    " datastore",
    " ams"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Craig Smith",
      "email": "vxdhost@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "~5.8.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.7.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Phpsa\\CamsGallery\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phpsa\\CamsGallery\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Phpsa\\CamsGallery\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
